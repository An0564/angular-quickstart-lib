{
  "name": "angular-quickstart-lib",
  "version": "1.0.0",
  "description": "Library QuickStart package.json from the documentation, supplemented with testing support",
  "main": "bundles/hero-profile.umd.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {},
  "scripts": {
    "prebuild": "npm run clean",
    "build": "ngc && node inline-resources.js \"src/*\" && rollup -c rollup-config.js",
    "clean": "rimraf bundles/ src/*.d.ts src/*.js src/*.js.map src/*.metadata.json src/*.ngsummary.json src/*.ngfactory.ts src/*.ngstyle.ts",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "peerDependencies": {
    "@angular/core": ">=2.0.0 <5.0.0 || >=4.0.0-beta <5.0.0"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.8",
    "@angular/compiler-cli": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@types/node": "^6.0.46",
    "glob": "^7.1.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.36.0",
    "rxjs": "^5.0.1",
    "tslint": "^4.4.2",
    "typescript": "~2.0.10",
    "zone.js": "^0.7.2"
  }
}
